<script language=javascript>
//計算日期相差天數
function Date_substr()
{
//定義起始 年月日
    var StartDate=$( "#startDay" ).val();
//定義結束 年月日
    var EndDate=$( "#endDay" ).val();

    aa=(DateDifference(StartDate,EndDate));
		document.getElementById("datetotal").innerHTML=("共"+aa+"天"+"&nbsp&nbsp&nbsp&nbsp&nbsp")+("總金額:"+(aa*1000));
		


}
// 算出日期與日期間的差距有幾天
function DateDifference(StartDate,EndDate)
{

   var myStartDate = new Date(StartDate);
   var myEndDate = new Date(EndDate);

   // 天數，86400000是24*60*60*1000，除以86400000就是有幾天
     return (myEndDate - myStartDate)/ 86400000 ; 
}
</script>


