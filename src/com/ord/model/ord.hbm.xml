<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ord.model.OrdVO" table="ORD" >
        
        <id name="ord_no" column="ORD_NO">
            <generator class="com.ord.model.OrderIdentifierGenerator" />
        </id>
        
        <property name="seller_no" column="SELLER_NO" />
        <property name="cust_no" column="CUST_NO" />
        <property name="address" column="ADDRESS" />
        <property name="ord_date" column="ORD_DATE"/>
        <property name="total" column="TOTAL" />
        <property name="score" column="SCORE" />
        <property name="status" column="STATUS" />
        
    </class>
</hibernate-mapping>