<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.content.model.ContentVO" table="content" >
        
        <id name="cont_no" column="cont_no">
            <generator class="com.content.model.ContentGenerator" />
        </id>
        
        <property name="alb_no" column="alb_no" />
        <property name="upload_date" column="upload_date" />
        <property name="img" column="img" />
        <property name="vdo" column="vdo" />
        
        
    </class>
</hibernate-mapping>