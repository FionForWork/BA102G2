<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.order_detail.model.Order_detailVO" table="ORDER_DETAIL" >

        <composite-id>
            <key-property name="ord_no" column="ORD_NO"/>
            <key-property name="pro_no" column="PRO_NO"/>
        </composite-id>        
        
        <property name="price" column="PRICE" />
        <property name="qty" column="QTY" />
        <property name="itemtot" column="ITEMTOT" />
        <property name="score" column="SCORE" />
        <property name="status" column="STATUS" />
        
    </class>
</hibernate-mapping>