<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.album.model.AlbumVO" table="album" >
        
        <id name="alb_no" column="alb_no">
            <generator class="com.album.model.AlbumGenerator" />
        </id>
        
        <property name="mem_no" column="mem_no" />
        <property name="name" column="name" />
        <property name="cover" column="cover" />
        <property name="create_date" column="create_date" />
        
        
    </class>
</hibernate-mapping>